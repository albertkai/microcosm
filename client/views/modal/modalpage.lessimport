@modal-overlay-color: rgba(0, 200, 255, .5);


.modal-page{
  position: fixed;
  width: 700px;
  height: 500px;
  border-top: 12px solid rgba(255, 255, 255, .5);
  border-bottom: 12px solid rgba(255, 255, 255, .5);
  border-left: 15px solid rgba(255, 255, 255, .5);
  border-right: 15px solid rgba(255, 255, 255, .5);
  top: 50%;
  left: 50%;
  z-index: @zindex-modal;
  margin-top: -250px;
  margin-left: -350px;
  .transition(.6s);
  visibility: hidden;
  opacity: 0;
  .rounded(5px);
  &._opened{
    visibility: visible;
    opacity: 1;
    .modal-insert{
      display: block;
    }
  }
  &.messages{
    width: 830px;
    height: 600px;
    margin-left: -415px;
    margin-top: -300px;
    .modal-insert{
      >.body{
        >aside{
          width: 300px;
          left: 0;
          .search{
            height: 10%;
            width: 100%;
            background: @gray-lighter;
            border-bottom: 1px solid white;
            input{
              font-size: 16px;
              border-bottom: 1px solid @gray-lighter;
              margin: 8px;
              width: 80%;
            }
            button{
              display: inline-block;
              position: absolute;
              width: 36px;
              height: 36px;
              padding: 0;
              top: 8px;
              right: 8px;
              background: white;
              color: @blue;
              font-size: 18px;
              &:hover{
                background: @blue;
                border: 1px solid @blue;
                color: white;
              }
            }
          }
          >ul{
            height: 80%;
            width: 100%;
            overflow-y: scroll;
            overflow-x: hidden;
            border-bottom: 1px solid white;
            >li{
              position: relative;
              height: 80px;
              font-size: 14px;
              >div{
                position: absolute;
                &:nth-child(1){
                  width: 25%;
                  left: 0;
                  top: 0;
                  >img{
                    height: 60px;
                    width: 60px;
                    .rounded(60px);
                    border: 2px solid white;
                    margin: 10px 8px;
                  }
                }
                &:nth-child(2){
                  width: 75%;
                  left: 25%;
                  top: 0;
                  >*{
                    margin: 0;
                    .transition(.2s);
                  }
                  >p, h5{
                    white-space: nowrap;
                    text-overflow: ellipsis;
                    overflow: hidden;
                    padding-right: 5px;
                  }
                  h5{
                    margin-top: 12px;
                    i{
                      font-size: 5px;
                      color: @blue;
                    }
                  }
                  p{
                    &:nth-child(2){
                      margin-top: 3px;
                    }
                    &:nth-child(3){
                      color: @gray-light;
                    }
                  }
                  .writing-message{
                    position: absolute;
                    width: 100%;
                    height: 40px;
                    top: 28px;
                    padding-left: 14px;
                    color: @blue;
                    font-size: 28px;
                    opacity: 0;
                    visibility: hidden;
                  }
                }
              }
              &._unwatched{
                background: rgba(0, 187, 255, 0.10);
              }
              &._active{
                background: white;
              }
              &._writing{
                >div:nth-child(2){
                  >p{
                    opacity: 0;
                  }
                  .writing-message{
                    visibility: visible;
                    opacity: 1;
                  }
                }
              }
            }
          }
          .filters{
            position: relative;
            height: 10%;
            width: 100%;
            .button-group{
              width: 90%;
              margin-left: 5%;
              position: relative;
              margin-top: 12px;
              button{
                position: absolute;
                top: 0;
                width: 33%;
                background: @gray-light;
                color:white;
                border: 0;
                font-size: 8px;
                -webkit-border-radius: 0;
                -moz-border-radius: 0;
                border-radius: 0;
                font-weight: 400;
                .transition(.4s);
                &._active, &:hover{
                  background: white;
                  color: @gray;
                }
                &:nth-child(1){
                  left: 0;
                  border-right: 1px solid @gray-lighter;
                  -webkit-border-top-left-radius: 30px;
                  -webkit-border-bottom-left-radius: 30px;
                  -moz-border-radius-topleft: 30px;
                  -moz-border-radius-bottomleft: 30px;
                  border-top-left-radius: 30px;
                  border-bottom-left-radius: 30px;
                }
                &:nth-child(2){
                  left: 33%;
                  border-right: 1px solid @gray-lighter;
                }
                &:nth-child(3){
                  left: 66%;
                  -webkit-border-top-right-radius: 30px;
                  -webkit-border-bottom-right-radius: 30px;
                  -moz-border-radius-topright: 30px;
                  -moz-border-radius-bottomright: 30px;
                  border-top-right-radius: 30px;
                  border-bottom-right-radius: 30px;
                }
              }
            }
          }
        }
        >div{
          position: absolute;
          top: 0;
          left: 300px;
          width: 500px;
          height: 100%;
          overflow: hidden;
          .grad{
            position: absolute;
            width: 100%;
            height: 30px;
            z-index: 10;
            background: -moz-linear-gradient(top,  rgba(255,255,255,1) 0%, rgba(255,255,255,0) 100%); /* FF3.6+ */
            background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(255,255,255,1)), color-stop(100%,rgba(255,255,255,0))); /* Chrome,Safari4+ */
            background: -webkit-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,0) 100%); /* Chrome10+,Safari5.1+ */
            background: -o-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,0) 100%); /* Opera 11.10+ */
            background: -ms-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,0) 100%); /* IE10+ */
            background: linear-gradient(to bottom,  rgba(255,255,255,1) 0%,rgba(255,255,255,0) 100%); /* W3C */
            filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#00ffffff',GradientType=0 ); /* IE6-9 */
            top: 10%;
            &.bottom{
              background: -moz-linear-gradient(top,  rgba(255,255,255,0) 0%, rgba(255,255,255,1) 100%); /* FF3.6+ */
              background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(255,255,255,0)), color-stop(100%,rgba(255,255,255,1))); /* Chrome,Safari4+ */
              background: -webkit-linear-gradient(top,  rgba(255,255,255,0) 0%,rgba(255,255,255,1) 100%); /* Chrome10+,Safari5.1+ */
              background: -o-linear-gradient(top,  rgba(255,255,255,0) 0%,rgba(255,255,255,1) 100%); /* Opera 11.10+ */
              background: -ms-linear-gradient(top,  rgba(255,255,255,0) 0%,rgba(255,255,255,1) 100%); /* IE10+ */
              background: linear-gradient(to bottom,  rgba(255,255,255,0) 0%,rgba(255,255,255,1) 100%); /* W3C */
              filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#00ffffff', endColorstr='#ffffff',GradientType=0 ); /* IE6-9 */
              top: auto;
              bottom: 10%;
              margin-bottom: -1px; //Fix
            }
          }
          #messages-top{
            position: relative;
            height: 10%;
            border-bottom: 1px solid @gray-lighter;
            a{
              font-size: 18px;
              line-height: 50px;
              padding-left: 10px;
              i{
                font-size: 6px;
              }
              sup{
                color: @blue;
              }
            }
            button{
              position: absolute;
              font-size: 16px;
              padding: 0;
              display: block;
              height: 31px;
              padding: 0 14px;
              line-height: 30px;
              top: 10px;
              right: 10px;
              span{
                position: relative;
                font-size: 10px;
                top: -2px;
              }
            }
          }
          #messages-cont{
            position: relative;
            height: 80%;
            overflow-y: scroll;
            overflow-x: hidden;
            .message{
              position: relative;
              margin: 40px 0;
              margin-left: 25px;
              &.audio{
                .body{
                  width: 380px;
                  height: 75px;
                  border: 0;
//                  background: @blue;
                  .gradient(@blue, @blue, @bluer-green);
                  .time{
                    position: absolute;
                    right: 10px;
                    top: 10px;
                    color:white;
                    font-size: 14px;
                  }
                  .audiowave{
                    position: absolute;
                    width: 306px;
                    height: 65px;
                    background: url(/img/audiowave.png);
                    left: 32px;
                    top: 5px;
                    opacity: .5;
                  }
                  .playback-position{
                    width: 30%;
                    height: 100%;
                    position: absolute;
                    top: 0;
                    left: 0;
                    background: rgba(255, 255, 255, .4);
                  }
                  .controls{
                    position: absolute;
                    left: 0;
                    top: 0;
                    width: 100%;
                    height: 100%;
                    >div{
                      width: 100%;
                      height: 100%;
                      position: absolute;
                      visibility: hidden;
                      left: 0;
                      top: 0;
                      text-align: center;
                      button{
                        visibility: hidden;
                        opacity: 0;
                      }
                      &:nth-child(1){
                        .play{
                          .scale(1.5);
                        }
                      }
                      &:nth-child(2){
                        .pause{
                          .scale(1.5);
                        }
                        .backward{
                          .translate(-30px, 0);
                          -webkit-transition-delay: 0;
                          -moz-transition-delay: 0;
                          -ms-transition-delay: 0;
                          -o-transition-delay: 0;
                          transition-delay: 0;
                        }
                        .forward{
                          .translate(30px, 0);
                          -webkit-transition-delay: 0;
                          -moz-transition-delay: 0;
                          -ms-transition-delay: 0;
                          -o-transition-delay: 0;
                          transition-delay: 0;
                        }
                      }
                      button{
                        position: relative;
                        top: 12px;
                        background: transparent;
                        width: 50px;
                        height: 50px;
                        line-height: 45px;
                        padding: 0;
                        .rounded(50px);
                        border-color: transparent;
                        &:hover{
                          border-color: @green;
                        }
                        &.forward, &.backward{
                          width: 30px;
                          height: 30px;
                          line-height: 27px;
                          font-size: 14px;
                          top: 9px;
                          margin: 0px 10px;
                        }
                      }
                    }
                    &._playing{
                      >div:nth-child(2){
                        visibility: visible;
                        button{
                          visibility: visible;
                          opacity: 1;
                        }
                        .pause{
                          .scale(1);
                        }
                        .forward, .backward{
                          .translate(0, 0);
                          -webkit-transition-delay: .4s;
                          -moz-transition-delay: .4s;
                          -ms-transition-delay: .4s;
                          -o-transition-delay: .4s;
                          transition-delay: .4s;
                        }
                      }
                    }
                    &._paused{
                      >div:nth-child(1){
                        visibility: visible;
                        .play{
                          opacity: 1;
                          visibility: visible;
                          .scale(1);
                        }
                      }
                    }
                  }
                }
              }
