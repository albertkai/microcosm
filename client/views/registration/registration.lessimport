#tests-container{
  .recto{
    .flip-cont{
      *{
        .transition(.8s);
      }
      h3{
        font-size: 32px;
        color: #00c0ff;
        font-weight: 100;
        width: 100%;
        text-align: center;
        margin: 20px 0 0 0;
        visibility: hidden;
        opacity: 0;
        .translate(0, -30px);
        &._visible{
          visibility: visible;
          opacity: 1;
          .translate(0, 0);
        }
      }
      >.row{
        >div{
          .make-md-column(6);
          img{
            width: 300px;
            margin: 33px 40px;
            visibility: hidden;
            opacity: 0;
            .scale(0);
            &._visible{
              visibility: visible;
              opacity: 1;
              .scale(1);
            }
          }
          p{
            font-weight: 200;
            color: @main-text-color;
            padding-right: 20px;
            .translate(0, -30px);
            visibility: hidden;
            opacity: 0;
            &._visible{
              visibility: visible;
              opacity: 1;
              .translate(0, 0);
            }
            &.stronger{
              font-size: 20px;
            }
          }
          &:nth-child(2){
            padding-top: 40px;
          }
        }
      }
      button{
        float: right;
        margin-right: 30px;
        visibility: hidden;
        opacity: 0;
        .translate(0, -30px);
        &._visible{
          visibility: visible;
          opacity: 1;
          .translate(0, 0);
        }
      }
    }
  }
}


