      width: 100%;
      height: 100%;
      top: 0;
      left: 0;
      -webkit-background-size: cover;
      background-size: cover;
      z-index: 1;
      .transition(.6s);
      opacity: 0;
      &._visible{
        opacity: 1;
      }
