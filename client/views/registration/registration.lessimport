  header{
    position: absolute;
    height: 60px;
    width: 100%;
    background: rgba(255, 255, 255, .4);
    top: 25px;
    z-index: @zindex-navbar-fixed;
    .logo{
      font-size: 24px;
      line-height: 60px;
      font-weight: 100;
      color: white;
    }
    .container{
      position: relative;
    }
    .reg-step{
      position: absolute;
      height: 100%;
      top: 0;
      left: 300px;
      >div{
        display: inline-block;
        width: 50px;
        height: 50px;
        margin-top: 5px;
        margin-right: 100px;
        line-height: 45px;
        text-align: center;
        color: white;
        border: 2px solid white;
        font-size: 28px;
        .rounded(60px);
        .transition(.4s);
        &._active{
          .scale(1.5);
          background: rgba(0, 255, 171, 0.70);
          border: 0;
        }
      }
    }
    .logout{
      position: absolute;
      height: 100%;
      top: 0;
      right: 0;
      line-height: 60px;
      font-size: 16px;
      a{
        color: white;
        .transition(.4s);
        &:hover{
          color: @blue-light;
          text-decoration: none;
        }
      }
    }
  }
  
  footer{
    position: absolute;
    background: @gray-darker;
    width: 100%;
    height: 5px;
    bottom: 0;
    line-height: 40px;
    font-size: 18px;
    z-index: @zindex-navbar-fixed;
  }

