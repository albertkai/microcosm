                            cursor: pointer;
                            font-weight: 200;
                            &:hover{
                              background: #eef5f7;
                            }
                            .choose-level{
                              height: 0;
                              .transition(.4s);
                              -webkit-transform-origin: center top;
                              -moz-transform-origin: center top;
                              -ms-transform-origin: center top;
                              -o-transform-origin: center top;
                              transform-origin: center top;
                              -webkit-transform: scaleY(0);
                              -moz-transform: scaleY(0);
                              -ms-transform: scaleY(0);
                              -o-transform: scaleY(0);
                              transform: scaleY(0);
                              .lang-level{
                                position: relative;
                                top: 3px;
                                width: 20px;
                                height: 20px;
                                cursor: pointer;
                                .rounded(30px);
                                background: @lang-level-low;
                                display: inline-block;
                                margin: 3px;
                                opacity: .7;
                                .transition(.4s);
                                &:hover{
                                  opacity: 1;
                                  .scale(1.3);
                                }
                                &:nth-child(2){
                                  background: @lang-level-lower-int;
                                }
                                &:nth-child(3){
                                  background: @lang-level-int;
                                }
                                &:nth-child(4){
                                  background: @lang-level-upper-int;
                                }
                                &:nth-child(5){
                                  background: @lang-level-pro;
                                }
                              }
                              &._hover{
                                height: 35px;
                                -webkit-transform: scaleY(1);
                                -moz-transform: scaleY(1);
                                -ms-transform: scaleY(1);
                                -o-transform: scaleY(1);
                                transform: scaleY(1);
                              }
                            }
                          }
                          hr{
                            margin: 10px 0;
                          }
                        }
                      }
                    }
                  }
                }
              }
          }

        }
        .recto{
          h1, h3, p{
            .transition(.7s);
            .translate(0, -30px);
            opacity: 0;
            visibility: hidden;
            &._visible{
              visibility: visible;
              opacity: 1;
              .translate(0, 0);
            }
          }
          h1{
            position: absolute;
            width: 100%;
            top: 155px;
            text-align: center;
            font-size: 42px;
            font-weight: 200;
            color: #53e7ff;
          }
          h3{
            position: absolute;
            top: 215px;
            text-align: center;
            width: 100%;
            font-size: 27px;
            font-weight: 400;
            color: #24ff9c;
          }
          p{
            position: absolute;
            top: 226px;
            padding: 50px;
            text-align: center;
            width: 100%;
            font-size: 20px;
            font-weight: 200;
            color: @gray-light;
          }
          .hello-cloud{
            .transition(1s);
            margin: 30px auto;
            width: 750px;
            height: 470px;
            background-image: url(/img/hello_cloud.png);
            -webkit-background-size: cover;
            background-size: cover;
            -webkit-transform-origin: center top;
            -moz-transform-origin: center top;
            -ms-transform-origin: center top;
            -o-transform-origin: center top;
            transform-origin: center top;
            opacity: 0;
            &._visible{
              opacity: 1;
            }
            &._small{
              .scale(.3);
            }
          }
        }
        .verso{
          -webkit-transform: rotate3d(1, 0, 0, -180deg);
          -moz-transform: rotate3d(1, 0, 0, -180deg);
          -o-transform: rotate3d(1, 0, 0, -180deg);
          transform: rotate3d(1, 0, 0, -180deg);
          overflow: hidden;
          .slider-wrap{
            position: absolute;
            top: 0;
            left: 0;
            width: 3000px;
            z-index: 1;
            -webkit-transition: all .5s cubic-bezier(0.765, 0.005, 0.260, 1.005);
            -moz-transition: all .5s cubic-bezier(0.765, 0.005, 0.260, 1.005);
            -ms-transition: all .5s cubic-bezier(0.765, 0.005, 0.260, 1.005);
            -o-transition: all .5s cubic-bezier(0.765, 0.005, 0.260, 1.005);
            transition: all .5s cubic-bezier(0.765, 0.005, 0.260, 1.005);
            .slide{
              position: relative;
              padding: 30px 0;
              float: left;
              height: 500px;
              width: 100%;

            }
          }
          .pins{
            position: relative;
            width: 330px;
            margin: 20px auto;
            z-index: 2;
            .pins-cont{
              .pin{
                position: absolute;
                width: 90px;
                height: 5px;
                border: 1px solid #ccc;
                .rounded(20px);
                cursor: pointer;
                &:nth-child(2){
                  left: 120px;
                }
                &:nth-child(3){
                  right: 0;
                }
                &.stripe{
                  -webkit-transition: all .5s cubic-bezier(0.765, 0.005, 0.260, 1.005);
                  -moz-transition: all .5s cubic-bezier(0.765, 0.005, 0.260, 1.005);
                  -ms-transition: all .5s cubic-bezier(0.765, 0.005, 0.260, 1.005);
                  -o-transition: all .5s cubic-bezier(0.765, 0.005, 0.260, 1.005);
                  transition: all .5s cubic-bezier(0.765, 0.005, 0.260, 1.005);
                  background: @blue;
                  left: 0;
                  border-color: @blue;
                }
              }
            }
          }
          .controls{
            position: absolute;
            bottom: 20px;
            height: 50px;
            z-index: 3;
            width: 100%;
            button{
              position: absolute;
              &.slide-further{
                right: 70px;
              }
              &.slide-back{
                left: 70px;
              }
              &:disabled{
                opacity: 0;
                visibility: hidden;
              }
            }
          }
        }

      }
    }
    &.testing{
      .background{
        background-image: url(/img/testing_bg.jpg);
      }
    }
  }

  .testing{

    .question-cont{

      .question{
        padding: 0 30px;
        h3, h4, p{
          text-align: center;
          font-family: @font-family-sans-serif;
